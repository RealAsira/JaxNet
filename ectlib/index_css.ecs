@response_contentType(text/css)
@pageFound.set(1)


:root {
    --box-shadow: 2px 5px 4px rgba(0,0,0,.25);
    --box-shadow-light: 2px 5px 4px rgba(0,0,0,.15);
    --box-shadow-inverse: -2px -5px 4px rgba(0,0,0,.25);
    --box-shadow-light-inverse: -2px -5px 4px rgba(0,0,0,.15);
    --box-border-color: lightgrey;
    --box-border-color-light: rgba(0,0,0,.05);
    --field-header-color: #555;
    --greyed-text-color: #888;
    --link-color: rgba(24,111,161,1);
    --link-color-hover: rgba(77,173,227,1);
    --link-color-white: white;
    --link-color-white-hover: #6fcffc;
    --neutral-blue-header: lightslategrey;
    --neutral-blue-header-light: rgba(215,225,233,1);
    --notice-highlight-color: #f5e091;
    --table-row-hover-color: #eeeeff;
    --scrollbar-track-color: white;
    --scrollbar-thumb-color:#dadada;
    --scrollbar-thumb-color-hover:#cacaca;
    --scrollbar-box-shadow: inset -1px 0px 0px #c9c9c9;
}



* {
    margin:0;
    padding:0;
    zoom:1;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;

    <ec:case if=@userSettings[DebugCSSLayout].is(true)>
        background-color:rgba(255,0,0,.05);
        outline:1px solid red;
    </ec:case>
}



button, .btn, .displayField, .titlebox_titlebar {
    font-family:calibri;
}



html body {min-height:100vh; min-height:100dvh; color:#333; overflow-x:hidden; max-width: 100vw; max-width:100dvw; scroll-behavior: smooth;}
html {scroll-behavior: smooth;}
hr {height:1px; background-color: #45484a; border:none;}
img {vertical-align: middle;}



::-webkit-scrollbar {
    width:8px;  /*vertical scrollbar width*/
    height:8px; /*horizontal scrollbar height*/
}

::-webkit-scrollbar-track {
    background: var(--scrollbar-track-color);
}

::-webkit-scrollbar-thumb {
    background: var(--scrollbar-thumb-color); transition: background .25s;
    box-shadow: var(--scrollbar-box-shadow);
} ::-webkit-scrollbar-thumb:hover {background: var(--scrollbar-thumb-color-hover);}

::-webkit-scrollbar-corner {
   background:var(--scrollbar-thumb-color); 
}

.titlebox_content::-webkit-scrollbar-track:horizontal {border-radius:5px}
.titlebox_content::-webkit-scrollbar-thumb:horizontal {border-radius:4px}














[desktop] {display:none !important;}
[mobile] {display:block !important;}
[mobile][flex] {display:flex !important;}
[mobile][grid] {display:grid !important;}
[mobile][inline] {display:inline !important;}
[mobile][inline-block] {display:inline-block !important;}

@media screen and (min-width: 982px){
    [desktop] {display:block !important;}
    [desktop][flex] {display:flex !important;}
    [desktop][grid] {display:grid !important;}
    [desktop][inline] {display:inline !important;}
    [desktop][inline-block] {display:inline-block !important;}
    [mobile], [mobile][grid], [mobile][flex], [mobile][inline-block], [mobile][inline] {display:none !important;}
}












a {
    color: var(--link-color-hover); text-decoration: none; transition: color .25s;

    &:not(.btn):hover {
        color: var(--link-color);
        & .icndiv {background-color: var(--link-color) !important;}
    }

    &.whiteLink {
        color: var(--link-color-white);
        &:hover {color:var(--link-color-white-hover);}
    }
}










.anchorify{
    cursor: pointer;

    &, & * {
        color: var(--link-color-hover);
        text-decoration:none;
        transition:color .25s;
    }
        
    &:hover, &:hover * {color: var(--link-color);}
    & .icndiv:hover {background-color:var(--link-color) !important;}
}











/* use shadowOnHover on elements that should have a shadow only when hovered over*/
.shadowOnHover {
    box-shadow: none; transition: border .25s, box-shadow .25s;
    &:hover {box-shadow: var(--box-shadow-light);}
}



/* use hoverShadow on elements that should always have a shadow, but need to increase shadow on hover */
.hoverShadow {
    box-shadow: var(--box-shadow-light); transition: box-shadow .25s;
    &:hover{box-shadow: var(--box-shadow);}
}



.hoverPointer:hover {cursor:pointer;}













.backButtonBar {
    display:block; width:100%; height:30px; line-height:30px;
    background-color:#fafafa;
    box-shadow:var(--box-shadow-light);
    border-radius:4px 18px 18px 50px;
    transition: background-color .25s, color .25s;

    &:hover {background-color:#f7f7f7;}
}











.barcodeInput {
    & .icndiv {background-color: var(--link-color)}

    &.barcodeInputActive {
        & .icndiv {background-color: var(--notice-highlight-color);}
    }
}












.breadcrumb {
    border: 1px solid #f0f0f0;
    border-radius: 5px;
    background-color: #f5f5f5;
    box-shadow: var(--box-shadow-light);
    width: calc(100% - 24px);
    margin: 7px auto;
    padding: 3px 7px;

    & li {display: inline-block;}
    & li:has(a:hover){& ~ strong, & ~ a color:var(--link-color);}
}

.collapseBreadCrumbLI li {margin-right:-3px;}
.noMarginBreadcrumb {margin: 7px 0px;}












.childLocationDiv {
    background-color: white;
    border: 1px solid #f0f0f0;
    border-radius: 5px;
    box-shadow: var(--box-shadow-light);
    cursor: pointer;
    width: max-content; height: 25px;
    padding: 0px 3px 0px 7px;
    margin: 7px 3px;
    display: inline-block;
    transition: background-color .25s;
    
    &:hover {background-color: white;}

    & span {
        line-height: 21px; color: var(--link-color); transition: color .25s;

        & .icndiv {
            background-color: var(--link-color) !important;
            transition: background-color .25s;
        }

        &:hover {
            color: var(--link-color-hover);
            & .icndiv {background-color: #6c7174;}
        }
    }
}













.collectionBanner {
    padding: 9px;

    & .collectionBanner-img {height:58px; display:inline-block; vertical-align:top;}
    & .collectionBanner-txt {
        display:inline-block; vertical-align: middle; margin-left:13px; max-width:calc(100% - 75px); font-family:arial; font-size: .9rem;
        & h2, & span {display:block;}
    }
}










.collectionCard {
    display:inline-block;
    width: clamp(256px, 17.5vw, 336px);
    width: clamp(256px, 17.5dvw, 336px);
    height:58px;
    padding:7px;
    vertical-align:middle;

    background-color:#fafafa;
    box-shadow: var(--box-shadow);
    border-radius: 7px;
    border-left: 1px solid rgba(0,0,0,.05);

    & .collectionCard-img, & .collectionCard-img img {display:inline-block; height:44px; margin-top:-36px;}
    & .collectionCard-name {display:inline-block; width:calc(100% - 44px - 14px); max-width:32ch; line-height:44px; white-space:nowrap; overflow-x:hidden; text-align:center; font-family:arial; font-size:.9rem;}

    @media screen and (max-width:721px) {
        & {width:100%;}
        & .collectionCard-name {font-size:.8rem; max-width:53ch;}
    }
}











.displayFields {display:flex; flex-wrap:wrap; justify-content:start;}

.displayField {
    display:inline-block; margin:4px 12px;

    & .displayField-header {font-weight:bold; color:var(--field-header-color); border-bottom:1px solid var(--field-header-color);}
    &, & * {white-space:nowrap;}
}










.dragTarget.over {
    background-color: #3d3d3d;
    outline:1px dotted #666;
}










.dropdown {
	position:relative;
	&:hover .dropdown-content, .dropdown-content:hover {display:block;}
    &:has(ul.dropdown-menu:empty) {display:none;}
}

.dropdown-content {
    display:none;
    position:absolute;
    background-color: white;
    color: black; /* default option if not a link*/
    box-shadow:var(--box-shadow-light);
    border:1px solid var(--box-border-color);
    border-radius:7px;
    line-height:16px;
    text-align:left;
    width:200px;
    min-height:40px;
    top:38px;
    right:0px;

    & ul {
        list-style-type:none;

        & li:first-child {border-radius: 7px 7px 0px 0px;}
        & li:last-child {border-radius:0px 0px 7px 7px; padding-bottom:13px;}

        & li {
            transition: background-color .25s, color .25s;
            padding:13px 0px 5px 13px;
            width:100%;
            margin:0;

            &:hover {background-color: #f0fbff;}
        }
    }
}














.highlightContainer {
    display:inline-block;
    padding: 0px 11px 11px 0px;

    & .highlight {
        & ol, & ul {margin-left: 20px;}

        display:flex; 
        flex-flow: column wrap;
        justify-content:center;
        align-items: center;

        width: clamp(256px, 17.5vw, 336px);
        width: clamp(256px, 17.5dvw, 336px);
        min-height:100px;
        max-height:600px;
        overflow-y: auto;
        overflow-x: hidden;
        padding:11px;

        background-color:#fafafa;
        box-shadow: var(--box-shadow);
        border-radius: 7px;
        border-left: 1px solid rgba(0,0,0,.05);

        & .highlight-img, & .highlight-img img {display:block; max-width: calc(100% - 13px); margin:5px auto; border-radius:5px;}
        & .highlight-title {text-align: center; font-family:arial; font-size: .95rem; font-weight:bold; color:#111; margin:5px 0px;}
        & .highlight-content {text-align: justify; color:#333; margin:5px 0px; & img {display:block; max-width: calc(100% - 13px); margin:5px auto; border-radius:5px;}}

        &.highlightType0  {& .highlight-img {order:1;}   & .highlight-title{order:2;}   & .highlight-content {order:3;}}  /*    render error                  */
        &.highlightType1  {& .highlight-img {order:1;}   & .highlight-title{order:2;}   & .highlight-content {order:3;}}  /*    document or article           */
        &.highlightType2  {& .highlight-img {order:1;}   & .highlight-title{order:2;}   & .highlight-content {order:3;}}  /*    collection                    */
        &.highlightType3  {& .highlight-img {order:1;}   & .highlight-title{order:2;}   & .highlight-content {order:3;}}  /*    product new or improved       */
        &.highlightType4  {& .highlight-img {order:1;}   & .highlight-title{order:2;}   & .highlight-content {order:3;}}  /*    product showcase              */
        &.highlightType5  {& .highlight-img {order:1;}   & .highlight-title{order:2;}   & .highlight-content {order:3;}}  /*    sale or promotion             */
        &.highlightType6  {& .highlight-img {order:1;}   & .highlight-title{order:2;}   & .highlight-content {order:3;}}  /*    upcoming event                */
        &.highlightType7  {& .highlight-img {order:1;}   & .highlight-title{order:2;}   & .highlight-content {order:3;}}  /*    miscellaneous                 */
        &.highlightType8  {& .highlight-img {order:1;}   & .highlight-title{order:2;}   & .highlight-content {order:3;}}  /*    news                          */
        &.highlightType9  {& .highlight-img {order:1;}   & .highlight-title{order:2;}   & .highlight-content {order:3;}}  /*    employee announcement         */
        &.highlightType10 {& .highlight-img {order:1;}   & .highlight-title{order:2;}   & .highlight-content {order:3;}}  /*    employee event or holiday     */
        &.highlightType11 {& .highlight-img {order:1;}   & .highlight-title{order:2;}   & .highlight-content {order:3;}}  /*    employee hired or promoted    */
        &.highlightType12 {& .highlight-img {order:1;}   & .highlight-title{order:2;}   & .highlight-content {order:3;}}  /*    employee miscellaneous        */
        &.highlightType13 {& .highlight-img {order:1;}   & .highlight-title{order:2;}   & .highlight-content {order:3;}}  /*    employee news                 */
        &.highlightType14 {& .highlight-img {order:1;}   & .highlight-title{order:2;}   & .highlight-content {order:3;}}  /*    employee recognition          */
        &.highlightType15 {& .highlight-img {order:1;}   & .highlight-title{order:2;}   & .highlight-content {order:3;}}  /*    employee retiring             */
        &.highlightType16 {& .highlight-img {order:1;}   & .highlight-title{order:2;}   & .highlight-content {order:3;}}  /*    employee wellness             */

        @media screen and (max-width:721px) {
            width:100%;
        }
    }
}













/* formfields for ec:inputs */
input, textarea {accent-color: var(--link-color);}

.aceEditor_editor {height:500px;}

.formfield {
    display:inline-block;

    & label {
        display:inline-block;
        font-weight:bold;
        color:#656565;
        font-size:.9rem;
    }

    & input, & textArea {
        display:inline-block;
        width:auto;
        min-width:169px;
        height:25px;
        padding:1px 7px;
        margin:3px 3px 3px 0px;
        background-color:white; color:#333;
        border:1px solid var(--box-border-color);
        border-radius:5px; 

        &[disabled] {background-color:#f1f1f1;}
        &::placeholder {color:#444;}
        /*&:autofill {box-shadow:0 0 0px 50px #45484a inset; text-fill-color: white;}    USED IN css_dark but default is acceptable for css_light*/
        /*&:-webkit-autofill {box-shadow:0 0 0px 50px #45484a inset; -webkit-text-fill-color: white;}    USED IN css_dark but default is acceptable for css_light*/
    }

    &.aceEditor {width:100%;}
    &.checkbox input {min-width:auto; height:auto; transform:translateY(2px);}
    &.color input {width:50px; max-width:none; min-width:0px;}
    &.file input {border:none;}
    &.money input {width:100px; max-width:none; min-width:0px; height:25px;}
    &.number input {width:80px; max-width:none; min-width:0px; height:21px;}
    &.range label, &.range span {transform:translate(0px, -11px);}
    &.search input {height:29px;}
    &.select select {padding:1px 7px; margin: 3px 3px 3px 0px; height:25px; min-width:169px; max-width:185px; border-radius:5px;}
    /*&.select select, &.select select option {background-color:#45484a; color:white;}    USED IN css_dark but default is acceptable for css_light*/

    &.textArea {width:100%;}
    &.textArea > p > label {margin-bottom:5px;}
    &.textArea textarea {height:160px; width:100%; max-width:100%; max-height:668px; padding:5px 7px;}

    &.textField {max-height:710px; overflow-y:hidden;}
    &.textField > label {margin-bottom:5px;}
    &.textField, &.html {width:100%;}
    & .tox-promotion {display:none !important;} /*  hides tinyMCE upgrade button  */

    &.daterange1 .daterangeinput1{
        & {width:175px;}
        &.daterangeinput1_start {border-radius:5px 0px 0px 5px;}
        &.daterangeinput1_end {border-radius:0px 5px 5px 0px; margin-left:-5px;}
        &.daterangeinput1_text::placeholder {text-align:center;}
        &.daterangeinput1.daterangeinput1_text::placeholder {color:#333; white-space:pre;}
    }

    &.daterange2 .daterangeinput2 {
        &.daterangeinput2_start, &.daterangeinput2_end {width:84px; min-width:auto;}
        &.daterangeinput2_start {border-radius:5px 0px 0px 5px; border-right:none; margin-right:-4px;}
        &.daterangeinput2_end {border-radius:0px 5px 5px 0px; border-left:none; margin-left:-4px;}
    }
    &.daterange2 .thru-mark, &.numberRange .thru-mark {display:inline-block; width:min-content; height:25px; line-height:18px; font-size:1.3em; background-color:white; border-top:1px solid var(--box-border-color); border-bottom:1px solid var(--box-border-color);}
    &.daterange2 .safari-thru-mark, &.numberRange .safari-thru-mark {line-height:16px !important;}

    &.numberRange .numberRangeInput {
        &.numberRangeInput_lower, &.numberRangeInput_upper {width:104px; min-width:auto; text-align:center;}
        &.numberRangeInput_lower {border-radius:5px 0px 0px 5px; border-right:none; margin-right:-4px;}
        &.numberRangeInput_upper {border-radius:0px 5px 5px 0px; border-left:none; margin-left:-4px;}
    }
}

.placeHolderLeft::placeholder {text-align:left !important;}
.placeHolderCenter::placeholder {text-align:center !important;}
.placeHolderRight::placeholder {text-align:right !important;}













.xHintContainer {
    display:inline-block;
    position:relative;
    transform:translate(3px, -3px);
    scale: .8;

    & .xHint {
        width:18px; height:18px;
        text-align:center; line-height:17px;
        border-radius:50%; 
        border:1px solid var(--field-header-color); 
        color: var(--field-header-color);
        transition: color .25s, border .25s;
    }
    
    & .xHint:hover + .xHintContent, & .xHintContent:hover {
        opacity: 1;
        pointer-events: all;
    }

    & .xHintContent {scale:1.25;}
}


.xHintContent {
    &.isVisible{opacity:1}
    
    opacity: 0; z-index:998;
    transition: opacity .3s;
    pointer-events: none;
    width:220px; height:min-content;
    min-height:24px;
    padding:2px 4px;
    position:absolute;
    scale: 1;
    
    text-align:left;
    font-family:calibri;
    font-weight:normal;
    font-size:14px !important;
    text-wrap:wrap;
    color: white;

    border:1px solid rgba(6,26,26,1);
    border-radius:5px;
    background-color:rgba(16,36,36,.9);
    box-shadow: var(--box-shadow-light);


    &[mouseZone='0'], &[mouseZone='5'] {      /*  has mouseZone, is 0, is 5  */
        bottom:34px; transform:translateX(-50%); left:200%;
        &::after {border-color: rgba(6,26,26,1) transparent transparent transparent; top: 100%; left: 50%; transform:translateX(-50%);}
    }  /*  old default positioning, works for zone 5  */


    /*  ALTERNATE ZONE POSITIONING */
    /*  top row  */
    &[mouseZone='1'] {top:34px;   transform:translateX(4.5%);                   &::after {border-color: transparent transparent rgba(6,26,26,1) transparent; bottom: 100%; left: 00%;  transform:translateX(50%);}}
    &[mouseZone='2'] {top:34px;   transform:translateX(-50%); left:200%;          &::after {border-color: transparent transparent rgba(6,26,26,1) transparent; bottom: 100%; left: 50%;  transform:translateX(-50%);}}
    &[mouseZone='3'] {top:34px;   transform:translateX(-78.5%);  &::after {border-color: transparent transparent rgba(6,26,26,1) transparent; bottom: 100%; left: 100%; transform:translateX(-150%);}}

    /*  middle row, bottom row  */
    &[mouseZone='7'], &[mouseZone='4'] {bottom:34px; transform:translateX(4.5%);                  &::after {border-color: rgba(6,26,26,1) transparent transparent transparent; top: 100%; left: 00%;  transform:translateX(50%);}}
    &[mouseZone='8'], &[mouseZone='6'] {bottom:34px; transform:translateX(-78.5%); &::after {border-color: rgba(6,26,26,1) transparent transparent transparent; top: 100%; left: 100%; transform:translateX(-150%);}}
    

    &::after {
        content: "";
        border-width: 9px;
        border-style: solid;
        position:absolute;
    }
}














.ecloader {
    opacity:0; transition: opacity .25s;
    position: absolute;
    z-index: 997;
    top: 0;
    left: 0;
    width:100%;
    height:100%;
    border-radius:5px;
    cursor:wait;
    background:rgba(0,0,0,.2);

    &.opened {opacity:1;}
    &.bigLoader {position:fixed; z-index:1000;}
}


.loader-circle {
    border: 12px solid #f3f3f3;
    border-radius: 50%;
    display:block; margin:auto;
    width: 90px;
    height: 90px;
    margin-bottom:19px;
    animation: spin linear infinite;
}
  
@keyframes spin {
    0% {transform: rotate(0deg);}
    100% {transform: rotate(360deg);}
}













.notice {
    background-color:var(--notice-highlight-color); display:inline; color: black; padding:0px 2px;
    &:empty {display:none;}
}

#alerts{
    display:flex;
    flex-wrap:wrap;
    flex-direction:column;
    justify-content:end;
    align-items:end;
    position:fixed;
    bottom:0; right:0;
    width:80%; max-width:310px;
    height:100%;
    z-index:999;
    background:transparent;
    pointer-events:none;
    transition: all .5s ease;
}

.notification {
    display:block;
    transform: translatex(310px);
    transition: transform .5s; 
    pointer-events:auto;
    width:100%;
    padding:5px;
    margin:0px 0px 19px 0px;
    z-index:999;
    background-color:#fafafa;
    box-shadow:var(--box-shadow-light);
    border: 1px solid var(--box-border-color);
    border-right: 0px;
    border-radius: 5px 0px 0px 5px;

    &.opened {transform: translatex(0px);}
    &.closed {display:none;}

    & span {
        display:inline-block;
        width: calc(100% - 27px);
        margin-left:8px;
        font-size:.9rem;
        overflow: hidden;
        text-align: left;
        vertical-align: middle;

        & img {max-width: calc(100% - 27px);}
    }
}












.pageNavigator {
    display:flex; flex-wrap:wrap;
    width:max-content; margin:auto;

    & a {color:var(--link-color); padding:3px; margin:0px 7px; border-radius:3px;}
    & .currentPage {background-color:var(--link-color); color:white; transition:background-color .25s;}
    & .currentPage:hover {background-color:var(--link-color-hover); color:white;}
}











.quantityBadge {
    display:inline-block;
    min-width:18px;
    height:16px;
    line-height:14px;
    vertical-align:middle;
    margin:-2px 2px 0px 2px;
    padding:1px 9px;
    border-radius:50px;

    color:white !important;
    background-color:#999;
    transition: background-color .25s;
    
    text-align:center;
    font-weight:bold;
    font-family:sans-serif;
    font-size:.75rem;
}

a:hover .quantityBadge {
    background-color:#888;
}











.quickNotification {
    display:block;
    border-left:1px solid rgba(0,0,0,.05);
    border-top:1px solid rgba(0,0,0,.05);
    border-right:1px solid rgba(0,0,0,.05);
    border-radius:5px;
    box-shadow:var(--box-shadow);
    padding:5px; margin:2px 0px 7px 0px;

    /*& .quickNotification-options {}
    & .quickNotification-content {}*/
}











#relatedLinks {
    & a, & button {
        & .icndiv {background-color:var(--link-color-hover) !important;}
        &:hover .icndiv {background-color:var(--link-color) !important;}
    }
    
    & button:hover {color:var(--link-color);} /*  allow color change  */
}












table.tbl {
    display:table;
    width:96%;
    margin:9px auto 18px auto;
    padding:18px;
    border-collapse:collapse;
    border:1px solid #e4e4e4;
    border-top:0;
    overflow:auto;
    font-size: .9rem;
    line-height:1rem;

    & * {white-space: nowrap;}

    & tr {
        transition: background-color .25s;

        &:nth-of-type(even){background-color:white;}
        &:nth-of-type(odd){background-color:#f3f3f3;}
        &:nth-child(1){
            text-align:left;
            background-color:#f3f3f3 !important;
            height:1.4rem;
            border-bottom:1px solid #e4e4e4;
        }

        &:hover {background-color:var(--table-row-hover-color);}
