/*  similar to bootstrap button  */
/*  primary (blue), secondary (grey), complimentary (orange), purple, success (green), warning (yellow), danger (red), info (teal)  */
button, .btn {
    display:inline-block;
    padding:5px; margin:5px 5px 5px 0px;
    border:1px solid var(--box-border-color);
    background-color:rgba(244,244,244,1);
    border-radius:5px;
    min-width:32px;
    height:32px;
    white-space: nowrap;
    font-weight:bold;
    transition:background-color .25s, border .25s, color .25s;

    &:hover {
        cursor:pointer;
        background-color:rgba(236,236,236,1);
    }
}

.btn {
    box-shadow:var(--box-shadow-light);
}

.btn-sml {
    padding: 1px 5px; margin: 0px;
    border-radius: 3px;
    min-width:8px; height:auto;
}

a.btn:not(.btn-primary, .btn-secondary, .btn-complimentary, .btn-purple, .btn-success, .btn-warning, .btn-danger, .btn-info){color:inherit;}

.btn-primary, .btn-secondary, .btn-complimentary, .btn-purple, .btn-success, .btn-warning, .btn-danger, .btn-info {
    color:white;
    font-weight:bold;
    transition:background-color .25s, border .25s, color .25s;
}

.btn-primary {
    background-color:#085891;
    border:1px solid #085891;
}

.btn-primary:hover {
    background-color:#0c7bcb;
    border:1px solid #0c7bcb;
}

.btn-secondary {
    background-color:#757575;
    border:1px solid #757575;
}

.btn-secondary:hover {
    background-color:darkgrey;
    border:1px solid darkgrey;
}

.btn-complimentary {
    background-color: #d17d00;
    border:1px solid #d17d00;
}

.btn-complimentary:hover {
    background-color: #ff9a03;
    border:1px solid #ff9a03;
}

.btn-purple {
    background-color: #8900b3;
    border:1px solid #8900b3;
}

.btn-purple:hover {
    background-color: #c300ff;
    border:1px solid #c300ff;
}

.btn-success {
    background-color:green;
    border:1px solid green;
}

.btn-success:hover {
    background-color:#00a300;
    border:1px solid #00a300;
}

.btn-warning {
    background-color:#b39300;
    border:1px solid #b39300;
}

.btn-warning:hover {
    background-color:#dab300;
    border:1px solid #dab300;
}

.btn-danger {
    background-color:#aa0000;
    border:1px solid #aa0000;
}

.btn-danger:hover {
    background-color:#ed0000;
    border:1px solid #ed0000;
}

.btn-info {
    background-color:var(--link-color);
    border:1px solid var(--link-color);
}

.btn-info:hover {
    background-color:var(--link-color-hover);
    border:1px solid var(--link-color-hover);
}










