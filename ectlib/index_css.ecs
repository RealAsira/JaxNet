.tDialog {
    opacity:1;
    cursor:wait;
    pointer-events:auto;
    transition: opacity .25s;
    position:fixed;
    height:100vh;
    height:100dvh;
    width:100vw;
    top:0;
    margin:0;
    padding:0;
    z-index:996;
    background-color:rgba(0,0,0,.6);

    &.closed {opacity:0; pointer-events:none;}
    &.opened {
        & .tDialog-content {transform: translatey(0%);}
    }

    & .tDialog-content {
        transform: translatey(calc(-100% - 21px));
        transition: transform .3s;
        width:96%;
        min-width:350px;
        min-height:300px;
        max-height:calc(92vh + 74px);
        margin:auto;
        margin-top:21px;
        padding:13px;
        border-radius:9px;
        background-color:#fafafa;
        box-shadow:var(--box-shadow-light);
        cursor:auto;

        @media screen and (min-width: 982px){
            & {width: 45%;}
        }
    }

    & .tDialog-header {
        height: 23px;
        position: absolute:
        top:0; left:0; right:0;

        & h, & h1, & h2, & h3, & h4, & h5, & h6, & a {display:inline-block;}
    }

    & .tDialog-body {
        max-height:calc((92vh + 74px) - 42px - 23px - 26px);
        padding-bottom:51px;
        overflow:auto;
    }

    & .tDialog-footer {
        height:42px;
        width:100%;
        border-radius:9px;
        background-color:#fafafa;
        position:absolute;
        bottom:0;
        right:0;
        left:0;
        padding:0px 13px;
        margin-bottom:9px;

        &  #tDialog-footer-buttons {float:right;}
    }
}












.titlebox {
    width:95%;
    min-width:300px;
    margin:0px auto 13px auto;
    background-color:#fafafa;
    box-shadow: var(--box-shadow);
    border-radius:5px;
    border-left:1px solid rgba(0,0,0,.05);

    & .titlebox_content {
        overflow-x:auto;
        padding:5px;
        & .titlebox {margin-top: 13px;}
    }

    & .titlebox_titlebar {
        display:flex;
        justify-content:space-between;
        width:auto;
        min-height:21px; line-height:25px;
        background-color: var(--neutral-blue-header-light);
        color: black;
        border-radius:5px 5px 0px 0px;
        padding:0px 5px;
        box-shadow:0px 1px 1px 0px rgba(0,0,0,.15);

        & .titlebox_titlebar_title {font-weight:bold; font-size:1.1rem; width:75%;}

        & .titlebox_titlebar_options {
            width:25%; min-width:76px; text-align:right; padding-right:7px;

            & a {
                text-decoration:none; color: var(--link-color);
                &:hover {color: var(--link-color-hover);}
                & .icndiv {transform:translateY(20%);}
            }
        }
    }

    &.dark > .titlebox_titlebar {background-color: var(--neutral-blue-header); color: white;}
}

.tComponent {
    position: relative; width: 100%; height: auto;
    &:has( > div.titlebox){width:96%; margin:0px auto 13px auto;}
    & .titlebox {width: 100%;}
}














