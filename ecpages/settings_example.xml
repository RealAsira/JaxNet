  </GlobalVariables>


  <ECPages>
    <ECLibrary>
      <Path>Z:\ecsite\eclibrary</Path>
      <DoCacheSource>False</DoCacheSource>
    </ECLibrary>

    <Debugger>
      <AutoResourceScan>False</AutoResourceScan>
    </Debugger>
  </ECPages>
