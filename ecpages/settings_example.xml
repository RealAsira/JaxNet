<?xml version="1.0" encoding="utf-8"?>
<Settings>
  <Connections>

    <Connection Name="local" Activate="False">
      <ProviderName>SQL Server</ProviderName>
      <Server>localhost</Server>
      <Database>localdb</Database>
      <Username>sa</Username>
      <Password>PASSWORD</Password>
    </Connection>

  <GlobalVariables>
